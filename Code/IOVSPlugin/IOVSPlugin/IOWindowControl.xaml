<UserControl x:Class="IOVSPlugin.IOWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vsshell="clr-namespace:Microsoft.VisualStudio.Shell;assembly=Microsoft.VisualStudio.Shell.15.0"
             Background="{DynamicResource {x:Static vsshell:VsBrushes.WindowKey}}"
             Foreground="{DynamicResource {x:Static vsshell:VsBrushes.WindowTextKey}}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="IntelliOverflow">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Margin="10" HorizontalAlignment="Center">IntelliOverflow</TextBlock>
            <TextBox Height="23" TextWrapping="Wrap" Text="Enter Search..."/>
            <Button Content="Search" Click="searchBtn_Click" Width="120" Height="80" x:Name="searchBtn" Margin="90,0"/>
            <ListBox x:Name="listBox" Height="140"/>
        </StackPanel>
    </Grid>
</UserControl>
